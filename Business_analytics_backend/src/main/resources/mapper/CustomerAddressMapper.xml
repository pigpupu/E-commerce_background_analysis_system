<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.businessanalysis.mapper.CustomerAddressMapper">

  <resultMap id="BaseResultMap" type="com.example.businessanalysis.domain.CustomerAddress">
    <id property="id" column="id" jdbcType="VARCHAR"/>
    <result property="province" column="province" jdbcType="INTEGER"/>
    <result property="city" column="city" jdbcType="INTEGER"/>
    <result property="detail" column="detail" jdbcType="VARCHAR"/>
    <result property="addTime" column="add_time" jdbcType="TIMESTAMP"/>
    <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    <result property="delFlag" column="del_flag" jdbcType="INTEGER"/>
  </resultMap>

  <sql id="Base_Column_List">
    id,province,city,
        detail,add_time,update_time,
        del_flag
  </sql>
</mapper>
