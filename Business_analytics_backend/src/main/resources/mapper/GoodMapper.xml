<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.businessanalysis.mapper.GoodMapper">

  <resultMap id="BaseResultMap" type="com.example.businessanalysis.domain.Good">
    <id property="id" column="id" jdbcType="VARCHAR"/>
    <result property="categoryId" column="category_id" jdbcType="INTEGER"/>
    <result property="brandId" column="brand_id" jdbcType="INTEGER"/>
    <result property="goodName" column="good_name" jdbcType="VARCHAR"/>
    <result property="productWeightG" column="product_weight_g" jdbcType="DECIMAL"/>
    <result property="productLengthCm" column="product_length_cm" jdbcType="DECIMAL"/>
    <result property="productHeightCm" column="product_height_cm" jdbcType="DECIMAL"/>
    <result property="productWidthCm" column="product_width_cm" jdbcType="DECIMAL"/>
    <result property="image" column="image" jdbcType="VARCHAR"/>
    <result property="price" column="price" jdbcType="DECIMAL"/>
    <result property="addTime" column="add_time" jdbcType="TIMESTAMP"/>
    <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    <result property="delFlag" column="del_flag" jdbcType="INTEGER"/>
  </resultMap>

  <sql id="Base_Column_List">
        id,category_id,brand_id,
        good_name,product_weight_g,product_length_cm,
        product_height_cm,product_width_cm,image,
        price,add_time,update_time,
        del_flag
  </sql>

</mapper>
